*** The function \texttt{solveBubbles} defined in the following
*** \texttt{VIEW-BUBBLE-PARSING} module parses the bubbles in a set of preview
*** maps. It takes two modules, the signature of the view's source theory,
*** with the variables declared in the view, to parse the source term in the
*** term maps, and the target theory, with the mappings of the variable 
*** declarations in the view, to parse the target terms.

fmod VIEW-BUBBLE-PARSING is
  pr BUBBLE-PARSING .
  pr PRE-VIEW .

  var PVMAPS : Set`(PreViewMap`) .
  var VMAP : ViewMap .
  vars T T' : Term .
  vars M M' : Module .
  var U : Unit .
  var QIL : QidList .

  op solveBubbles : Set`(PreViewMap`) Unit Unit -> Set`(ViewMap`) .

  eq solveBubbles(PVMAPS, U, error(QIL)) = none .
  eq solveBubbles(PVMAPS, error(QIL), U) = none .
  ceq solveBubbles((VMAP, PVMAPS), M, M')
    = (VMAP, solveBubbles(PVMAPS, M, M')) 
    if not (VMAP :: TermMap) .
  eq solveBubbles((preTermMap(T, T'), PVMAPS), M, M')
    = (termMap(
         solveBubbles(T, M, false, none, emptyDatabase),
         solveBubbles(T', M', false, none, emptyDatabase)),
      solveBubbles(PVMAPS, M, M')) .
  eq solveBubbles(none, M, M') = none .

