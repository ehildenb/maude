fmod EXT-QID is
  pr QID .
  pr CONVERSION .

  op conc : Qid Qid -> Qid .
  op index : Qid Int -> Qid .
  op strip : Qid -> Qid .
  op convert : Qid ~> Int .
  op _<_ : Qid Qid -> Bool .

  vars Q P : Qid .
  var M : Int .

  eq conc(Q, P) = qid(string(Q) + string(P)) .
  eq index(Q, M) = qid(string(Q) + string(M, 10)) .
  eq strip(Q) = qid(substr(string(Q), 1, length(string(Q)))) .
  eq convert(Q) = trunc(rat(string(Q), 10)) .
  eq Q < P = string(Q) < string(P) .
