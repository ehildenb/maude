  ceq unitInstAux(U, M, VMAPS, 
       parList(par X :: par(ME, PL), PL'), 
       PL'', (EIL (protecting par X :: par(ME, PL) .) EIL'), EIL'',
       QI | VE, VEPS, PL''', DB)
