  op timedUntilStable : Module Term Term Condition Bool Term Condition Bool
                                                       -> Termset .
  op timedUntilStable : Module Term Term Condition Bool Term Condition 
                                           Bool Bound -> Termset .
  op timedUntilStable : Module Term Term Condition Bool Term Condition Bool
                                   ComparisonOp Term -> Termset .
  op timedUntilStable : Module Term Term Condition Bool Term Condition 
