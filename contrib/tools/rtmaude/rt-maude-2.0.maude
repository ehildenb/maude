  var  C : Constant .
  vars QI QI' QI'' F L : Qid .
  var  QIL : QidList .
  var  S : Sort .
  vars SS SS' : SortSet .
  var  TP : Type .
  var  TPL : TypeList .
  var  IL : ImportList .
  var  SSDS : SubsortDeclSet .
  var  OPDS : OpDeclSet .
  var  MAS : MembAxSet .
  var  EqS : EquationSet .
  var  RlS : RuleSet .
