                else nil fi) 'in '\o
         eMetaPrettyPrint(getName(MOD)) '\c '\s '\n '\t
         eMetaPrettyPrint(MOD, TERM) qid("=>" + string(searchQid(Q))) '\s
         eMetaPrettyPrint(MOD, SEARCHPATTERN)'\n 
         '\c 'with 'mode 
         printMode(SOLVEDTICKMODE, MOD)   ': '\n '\o
         *** Here comes the real call:
         procUnTimedSearch2(modeify(MOD, SOLVEDTICKMODE),
                          TERM, SEARCHPATTERN, nil, 
                          if searchQid(Q) == ' then '* else searchQid(Q) fi, 
                          if searchQid(Q) == ' then 1 else unbounded fi, 0, 
                          BOUND)) 
      if (Q == 'utsearch_=>_.) or
         (Q == 'utsearch_=>*_.) or
         (Q == 'utsearch_=>+_.) or
         (Q == 'utsearch_=>!_.) .

  op procUnTimedSearch2 : Module Term Term Condition Qid Bound Nat
                          Bound -> QidList .
  *** procUnTimedSearch2(module, initterm, pattern, condition, 
  ***                    arrowkind, depthOfRewrites, solNo, noOfSolsSought)

  eq procUnTimedSearch2(MOD, TERM, SEARCHPATTERN, COND, Q, D, N, BOUND) =
       if 
         untimedMetaSearch(MOD, TERM, SEARCHPATTERN, COND, Q, D, N)
         :: ResultTriple
       then 
         ('\n  '\c 'Solution qid(string(N + 1, 10))
          '\n '\o eMetaPrettyPrint(MOD, getSubstitution(untimedMetaSearch(MOD, 
                      TERM, SEARCHPATTERN, COND, Q, D, N))) '\n 
          (if N + 1 < BOUND
           then procUnTimedSearch2(MOD, TERM, SEARCHPATTERN, COND, Q, D, 
                                 N + 1, BOUND)
           else nil fi)
          )
       else
       (if N == 0 then '\n '\c 'No 'solution '\o '\n 
              else  '\n  '\c 'No 'more 'solutions '\o '\n fi)
       fi . 


  *** Now, search with time limits:
  ceq procParsedTimedCommandTPL(Q, MOD, TERM, BOUND, SEARCHPATTERN, LIMIT, 
                                SOLVEDTICKMODE) =
        ('\n '\c 'Timed 'search 
               (if BOUND =/= unbounded    *** write [13] etc 
                then ('\s '`[ qid(string(BOUND, 10)) '`] '\s )
                else nil fi) 'in '\o 
         eMetaPrettyPrint(getName(MOD)) '\c '\s '\n '\t
         eMetaPrettyPrint(MOD, TERM) qid("=>" + string(searchQid(Q))) '\s
         eMetaPrettyPrint(MOD, SEARCHPATTERN) '\n '\c
         'in 'time commandToCompSymb(Q) 
         eMetaPrettyPrint(MOD, LIMIT) 'and 'with 'mode 
         printMode(SOLVEDTICKMODE, MOD)   ': '\n '\o
         *** Here comes the real call:
         procTimedSearch2(modeify(MOD, SOLVEDTICKMODE),
                          TERM, SEARCHPATTERN, nil, 
                          if searchQid(Q) == ' then '* else searchQid(Q) fi, 
                          if searchQid(Q) == ' then 1 else unbounded fi, 0, 
                          commandToComp(Q), LIMIT, BOUND)) 
      if (Q == 'tsearch_=>_in`time`<_.) or
         (Q == 'tsearch_=>*_in`time`<_.) or
         (Q == 'tsearch_=>+_in`time`<_.) or
         (Q == 'tsearch_=>!_in`time`<_.) or
         (Q == 'tsearch_=>_in`time`<=_.) or
         (Q == 'tsearch_=>*_in`time`<=_.) or
         (Q == 'tsearch_=>+_in`time`<=_.) or
         (Q == 'tsearch_=>!_in`time`<=_.) or
         (Q == 'tsearch_=>_in`time`>=_.) or
         (Q == 'tsearch_=>*_in`time`>=_.) or
         (Q == 'tsearch_=>+_in`time`>=_.) or
         (Q == 'tsearch_=>!_in`time`>=_.) or
         (Q == 'tsearch_=>_in`time`>_.) or
         (Q == 'tsearch_=>*_in`time`>_.) or
         (Q == 'tsearch_=>+_in`time`>_.) or
         (Q == 'tsearch_=>!_in`time`>_.) .

  
  
  *** Now for in-between searches!

  ceq procParsedTimedCommandTPCLCL(Q, MOD, TERM, BOUND,SEARCHPATTERN, COMP, 
                                   LIMIT, COMP', LIMIT', SOLVEDTICKMODE) =

        ('\n '\c 'Timed 'search 
               (if BOUND =/= unbounded    *** write [13] etc 
                then ('\s '`[ qid(string(BOUND, 10)) '`] '\s )
                else nil fi) 'in '\o 
         eMetaPrettyPrint(getName(MOD)) '\s '\n '\t
         eMetaPrettyPrint(MOD, TERM) qid("=>" + string(searchQid(Q))) '\s
         eMetaPrettyPrint(MOD, SEARCHPATTERN) '\n 
         '\c 'in 'time 'between 
         (if COMP == lt then '< 
          else (if COMP == le then '<= 
                else (if COMP == gt then '> else '>= fi) fi) fi)
         eMetaPrettyPrint(MOD, LIMIT) '\c 
         'and (if COMP' == lt then '< 
               else (if COMP' == le then '<= 
                     else (if COMP' == gt then '> else '>= fi) fi) fi)
         eMetaPrettyPrint(MOD, LIMIT') '\c 
        'and 'with 'mode 
         printMode(SOLVEDTICKMODE, MOD)  ': '\n '\o
         *** Here comes the real call:
         procTimedSearch2(modeify(MOD, SOLVEDTICKMODE),
                          TERM, SEARCHPATTERN, nil, 
                          if searchQid(Q) == ' then '* else searchQid(Q) fi, 
                          if searchQid(Q) == ' then 1 else unbounded fi, 0, 
                          COMP, LIMIT, COMP', LIMIT', BOUND))
      if (Q == 'tsearch_=>_in`time-interval`between__and__.) or 
         (Q == 'tsearch_=>*_in`time-interval`between__and__.) or 
         (Q == 'tsearch_=>+_in`time-interval`between__and__.) or 
         (Q == 'tsearch_=>!_in`time-interval`between__and__.) .

  op procTimedSearch2 : Module Term Term Condition Qid Bound Nat 
                        ComparisonOp Term ComparisonOp Term Bound -> QidList .

  eq procTimedSearch2(MOD, TERM, SEARCHPATTERN, COND, Q, D, N, COMP, LIMIT,
                      COMP', LIMIT', BOUND) =
       if 
         timedMetaSearch(MOD, TERM, SEARCHPATTERN, COND, Q, D, N, 
