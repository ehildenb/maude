  op flatModule : Unit -> Module .
  eq flatModule(error(QIL)) = error(QIL) .
  ceq flatModule(U)
    = if U :: FUnit
      then (fmod modNameToQid(getName(U)) is
               getImports(U)
               sorts eSortToSort(getSorts(U)) .
