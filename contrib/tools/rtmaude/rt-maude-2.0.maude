  eq up((mb T : S [AtS] .)) = 'mb_:_`[_`].[up(T), up(S), up(AtS)] .
  eq up((cmb T : S if EQCD [AtS] .)) 
    = 'cmb_:_if_`[_`].[up(T), up(S), up(EQCD), up(AtS)] .
