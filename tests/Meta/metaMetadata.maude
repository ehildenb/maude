
set show advisories off .

set protect BOOL off .

fmod FOO is
  sort Foo .
  ops a b : -> Foo [metadata "bla bla"] .
  eq a = b [metadata "hmm"] .
endfm

show mod .
show all .
red in META-LEVEL : upModule('FOO, false) .


