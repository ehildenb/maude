***
***	Now with the incompleteness arising from a variant equation.
***

mod BAR is
  sort Foo .
  op f : Foo Foo -> Foo [assoc] .
  ops g h i : Foo -> Foo .
  ops a b c d e : -> Foo .

vars X Y Z W : Foo .
  eq g(f(X, X)) = h(X) [variant].
  rl h(X) => i(X) [narrowing] .
endm

variant unify h(X) =? g(f(Y, Z, Z)) .

