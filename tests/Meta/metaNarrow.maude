***
***	Check handling of unification incompleteness.
***

mod FOO is
  sort Foo .
  op f : Foo Foo -> Foo [assoc] .
  ops g h : Foo -> Foo .
  ops a b c d e : -> Foo .

vars X Y Z W : Foo .
  rl g(f(X, X)) => h(X) [narrowing].
endm

*** 4 unifiers + incompleteness
unify f(X, X) =? f(Y, Z, Z) .

